{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "GCN",
    "dataset": "Building",

    "out_dir": "out/Building_edge_classification/",

    "params": {
        "seed": 41,
        "epochs": 400,
        "init_lr": 0.01,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.001,
        "print_epoch_interval": 5,
        "max_time": 24
    },

    "net_params": {
        "L": 3,
        "hidden_dim": 120,
        "out_dim": 5,
        "residual": false,
        "dropout": 0,
        "batch_norm": true,
        "self_loop": false,
        "n_classes": 2
    }
}